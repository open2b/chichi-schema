type user (
    first_name text(300)     -- First name
        aliases: name
    second_name text(300)    -- Second name
        default: 'abc'
    last_name  text(300)     -- Last name
    email      text(300)     -- Email
    addresses  array         -- Addresses
        aliases: billing_address
        street text(60)         -- Street
        city   text(20)         -- City
        cap    text(20)         -- CAP
    addresses  address array -- Addresses
    orders     order array
)
